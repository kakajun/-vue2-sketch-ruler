{
  "name": "vue3-sketch-ruler",
  "version": "1.3.4",
  "author": "kakajun <253495832@qq.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "lib",
    "types",
    "bin",
    "scripts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kakajun/vue3-sketch-ruler/tree/1x"
